{
  "name": "covid-assistant",
  "version": "1.0.0",
  "description": "A Chatbot aimed at making life during covid as simple as possible",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "client-install": "cd client && npm install",
    "project-install": "npm install && npm run client-install",
    "dev": "concurrently \"nodemon app\" \"npm start --prefix client\""
  },
  "author": "Sayan Das & Arya Sarkar",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.2",
    "react-particles-js": "^3.3.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
